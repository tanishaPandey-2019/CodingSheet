#include <iostream>
#include<climits>
using namespace std;

int main()
{
    int arr[5] = { 1 , 2 , 3 , 4 , 5};
    int n = 5;
    
    int minNum = INT_MAX;
    int maxNum = INT_MIN;
    
    for(int i=0;i<n;i++)
    {
        minNum = min(arr[i],minNum);
        maxNum = max(arr[i],maxNum);
    }
    
    cout<<minNum<<endl;
    cout<<maxNum<<endl;

    return 0;
}

